<h1>Add a new parcel</h1>

<form action="/parcels" method="post">
  <div class="form-group">
    <label for="parcel_name">Parcel name</label>
    <input id="n" name="parcel_name" class="form-control" type="text">
    <label for="parcel_dimensions">Parcel Dimensions</label>
    <input id="dimensions" name="dimensions" class="form-control" type="text">
    <label for="parcel_weight">Parcel Weight</label>
    <input id="weight" name="weight" class="form-control" type="text">
  </div>
  <button type="submit" class="btn">Go!</button>
</form>
